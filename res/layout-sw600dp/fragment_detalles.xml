<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fragment_detalles"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fragment_divider">

    <TextView
        android:id="@+id/textView_biblioteca_nomod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/textView_biblioteca"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="20dp"
        android:text="@string/textView_biblioteca"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    
    
    <TextView
        android:id="@+id/textView_biblioteca"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="80dp"
        android:gravity="center"
        android:layout_above="@+id/textView_sucursal_nomod"
        android:text="" />
    
     <TextView
        android:id="@+id/textView_sucursal_nomod"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:layout_above="@+id/textView_sucursal"
        android:text="@string/textView_sucursal"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    
    
    
    <TextView
        android:id="@+id/textView_sucursal"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="80dp"
        android:gravity="center"
        android:layout_above="@+id/textView_titulo_nomod"
        android:text="" />
    
    <TextView
        android:id="@+id/textView_titulo_nomod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView_titulo"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:text="@string/textView_titulo"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    
    <TextView
        android:id="@+id/textView_titulo"
        android:layout_width="320dp"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:layout_centerInParent="true"
        android:text="" />
    
    <TextView
        android:id="@+id/textView_fecha_nomod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_titulo"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:text="@string/textView_fecha"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    
    <TextView
        android:id="@+id/textView_fecha"
        android:layout_width="320dp"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/textView_fecha_nomod"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:text="" />

    

</RelativeLayout>
